<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE Tags SYSTEM "matroskatags.dtd">
<Tags>
    <Tag>                                         <!-- Movie Template -->
        <Targets>
            <TargetTypeValue>50</TargetTypeValue> <!-- 50 is used for MOVIE/EPISODE/CONCERT -->
        </Targets>
        <Simple>
            <Name>IMDB</Name>
            <String>tt$imdb</String>              <!-- Replace # with imdb id for that movie e.g. tt4154756 -->
        </Simple>
        <Simple>
            <Name>TMDB</Name>
            <String>movie/$tmdb</String>          <!-- Replace # with tmdb id for that movie e.g. movie/299536 -->
        </Simple>
    </Tag>
</Tags>