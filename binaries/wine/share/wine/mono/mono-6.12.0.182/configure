if test "x$with_spectre_mitigation" = "xyes"; then
   { $as_echo "$as_me:${as_lineno-$LINENO}: Compiler Spectre mitigation support checks" >&5
$as_echo "$as_me: Compiler Spectre mitigation support checks" >&6;}
   SPECTRE_CFLAGS=
   SPECTRE_INDIRECT_BRANCH_KIND=
   case "x$with_spectre_indirect_branch_choice" in
       xkeep) SPECTRE_INDIRECT_BRANCH_KIND=keep ;;
       xthunk) SPECTRE_INDIRECT_BRANCH_KIND=thunk ;;
       xinline) SPECTRE_INDIRECT_BRANCH_KIND=thunk-inline ;;
       xextern) SPECTRE_INDIRECT_BRANCH_KIND=thunk-extern ;;
       *) as_fn_error $? "Invalid indirect jump thunk kind ($with_spectre_indirect_branch_choice)" "$LINENO" 5 ;;
   esac

   SPECTRE_FUNCTION_RETURN_KIND=""
   case "x$with_spectre_function_return_choice" in
       xkeep) SPECTRE_FUNCTION_RETURN_KIND=keep ;;
       xthunk) SPECTRE_FUNCTION_RETURN_KIND=thunk ;;
       xinline) SPECTRE_FUNCTION_RETURN_KIND=thunk-inline ;;
       xextern) SPECTRE_FUNCTION_RETURN_KIND=thunk-extern ;;
       *) as_fn_error $? "Invalid function return thunk kind ($with_spectre_function_return_choice)" "$LINENO" 5 ;;
   esac

   as_CACHEVAR=`$as_echo "ax_cv_check_cflags__ -mindirect-branch=$SPECTRE_INDIRECT_BRANCH_KIND " | $as_tr_sh`
{ $as_echo "$as_me:${as_lineno-$LINENO}: checking whether C compiler accepts  -mindirect-branch=$SPECTRE_INDIRECT_BRANCH_KIND " >&5
$as_echo_n "checking whether C compiler accepts  -mindirect-branch=$SPECTRE_INDIRECT_BRANCH_KIND ... " >&6; }
if eval \${$as_CACHEVAR+:} false; then :
  $as_echo_n "(cached) " >&6
else

  ax_check_save_flags=$CFLAGS
  CFLAGS="$CFLAGS   -mindirect-branch=$SPECTRE_INDIRECT_BRANCH_KIND "
  cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

int
main ()
{

  ;
  return 0;
}
_ACEOF
if ac_fn_c_try_compile "$LINENO"; then :
  eval "$as_CACHEVAR=yes"
else
  eval "$as_CACHEVAR=no"
fi
rm -f core conftest.err conftest.$ac_objext conftest.$ac_ext
  CFLAGS=$ax_check_save_flags
fi
eval ac_res=\$$as_CACHEVAR
	       { $as_echo "$as_me:${as_lineno-$LINENO}: result: $ac_res" >&5
$as_echo "$ac_res" >&6; }
if eval test \"x\$"$as_CACHEVAR"\" = x"yes"; then :
   SPECTRE_CFLAGS="$SPECTRE_CFLAGS -mindirect-branch=$SPECTRE_INDIRECT_BRANCH_KIND"

else
  :
fi


   as_CACHEVAR=`$as_echo "ax_cv_check_cflags__ -mfunction-return=$SPECTRE_FUNCTION_RETURN_KIND " | $as_tr_sh`
{ $as_echo "$as_me:${as_lineno-$LINENO}: checking whether C compiler accepts  -mfunction-return=$SPECTRE_FUNCTION_RETURN_KIND " >&5
$as_echo_n "checking whether C compiler accepts  -mfunction-return=$SPECTRE_FUNCTION_RETURN_KIND ... " >&6; }
if eval \${$as_CACHEVAR+:} false; then :
  $as_echo_n "(cached) " >&6
else

  ax_check_save_flags=$CFLAGS
  CFLAGS="$CFLAGS   -mfunction-return=$SPECTRE_FUNCTION_RETURN_KIND "
  cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

int
main ()
{

  ;
  return 0;
}
_ACEOF
if ac_fn_c_try_compile "$LINENO"; then :
  eval "$as_CACHEVAR=yes"
else
  eval "$as_CACHEVAR=no"
fi
rm -f core conftest.err conftest.$ac_objext conftest.$ac_ext
  CFLAGS=$ax_check_save_flags
fi
eval ac_res=\$$as_CACHEVAR
	       { $as_echo "$as_me:${as_lineno-$LINENO}: result: $ac_res" >&5
$as_echo "$ac_res" >&6; }
if eval test \"x\$"$as_CACHEVAR"\" = x"yes"; then :
   SPECTRE_CFLAGS="$SPECTRE_CFLAGS -mfunction-return=$SPECTRE_FUNCTION_RETURN_KIND"

else
  :
fi


   if test "x$SPECTRE_CFLAGS" != "x" ; then
