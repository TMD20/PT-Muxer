		rm -f machine.config
		$LN_S $reldir/data/net_2_0/machine.config machine.config
		cd $depth
	 ;;
    "runtime/etc/mono/2.0/web.config":C)    depth=../../../..
		case $srcdir in
		[\\/$]* | ?:[\\/]* ) reldir=$srcdir ;;
		.) reldir=$depth ;;
