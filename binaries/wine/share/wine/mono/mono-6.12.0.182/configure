  HAS_EXTENSION_MODULE_TRUE=
  HAS_EXTENSION_MODULE_FALSE='#'
else
  HAS_EXTENSION_MODULE_TRUE='#'
  HAS_EXTENSION_MODULE_FALSE=
fi


